{
  "parser": "babel-eslint",
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "extends": [
    "standard",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "flowtype",
    "async-await"
  ],
  "rules": {
    "async-await/space-after-async": 1,
    "async-await/space-after-await": 1,
    "spaced-comment": 0,
    "no-fallthrough": 0,
    "padded-blocks": 0,
    "space-infix-ops": 0
  }
}
